<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!--

-->

<mapper namespace="kr.co.Dal.comm.mapper.CommMapper">

    <select id="commList" resultType="kr.co.Dal.comm.model.CommVO">
        SELECT *
        FROM bard_tb
    </select>

    <insert id="commInsert" parameterType="kr.co.Dal.comm.model.CommVO">
        insert into bard_tb
        (
         BARD_ID
        ,USER_ID
        ,BARD_TYPE
        ,BARD_TIT
        ,BARD_CN
        ,BARD_RDATE
        ,BARD_CNT
        ,BARD_LIKE_CNT
        ,BARD_STTS
        ,CATE_TYPE
        )
        values
        (
         #{BARD_ID}
        ,#{USER_ID}
        ,#{BARD_TYPE}
        ,#{BARD_TIT}
        ,#{BARD_CN}
        ,#{BARD_RDATE}
        ,#{BARD_CNT}
        ,#{BARD_LIKE_CNT}
        ,#{BARD_STTS}
        ,#{CATE_TYPE}
        )
    </insert>

</mapper>