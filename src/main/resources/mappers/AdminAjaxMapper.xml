    <select id="selectStoreList" resultType="kr.co.Dal.admin.model.AdminStoreVO">
        SELECT
            prod_id,
            prod_tit,
            prod_type,
            prod_cnt,
            prod_ori_price,
            prod_sale_price,
             prod_img,
             prod_stts,
             prod_like,
             prod_rdate,
             prod_udate,
             prod_cn_file
