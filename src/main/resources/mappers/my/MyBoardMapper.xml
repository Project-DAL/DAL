<mapper namespace="kr.co.Dal.my.mapper.MyBoardMapper">
    <select id="selectBoardList" resultType="kr.co.Dal.my.model.MyBoardVO">
        SELECT
            bard_id,
            user_id,
            bard_type,
            bard_tit,
            bard_cnt,
            bard_rdate,
            bard_stts,
            cate_type
        FROM
            bard_tb
    </select>



